{
  "version": 2,
  "builds": [
    {
        "installCommand": "npm install",
      "framework": "expressjs",
      "src": "app.js"
    },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      
      "src": "/(.*)",
      
      "dest": "/"
    }
  ]
}